{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "88d8a9ef-8fbe-4b5d-8961-9604453a9a74"
  },
  "newProjectRoot": "projects",
  "projects": {
    "mont-validators": {
      "projectType": "library",
      "root": "projects/mont-validators",
      "sourceRoot": "projects/mont-validators/src",
      "prefix": "mont",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mont-validators/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mont-validators/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mont-validators/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@analogjs/vitest-angular:test"
        }
      }
    }
  }
}
